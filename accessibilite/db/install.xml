<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/accessibilite/db" VERSION="20220220" COMMENT="XMLDB file for Moodle blocks/accessibilite"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_accessibilite" COMMENT="Table des configurations utilisateur">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="code" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="un_userid" TYPE="unique" FIELDS="userid"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
